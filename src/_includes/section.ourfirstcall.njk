{% for item in collections.sections %}
  {% if item.data.type === 'ourfirstcall' %}
    <div id="ourfirstcall" class="w-full min-h-screen flex flex-col justify-center items-center bg-teal-900">
      <div class="self-center text-center w-full xl:w-1/3 px-4 sm:px:0">
        <h2 class="font-bold tracking-widest text-gray-200 text-4xl mb-4">{{ item.data.title }}</h2>
        <div class="font-light text-gray-400 text-xl">
          {{ item.templateContent | safe }}
        </div>
      <div class="flex flex-row w-full justify-center pt-6">
        <a class="px-10 py-2 text-gray-200 bg-amber-600 rounded-full shadow-md text-lg hover:bg-gray-800 hover:border-red" href="https://calendly.com/terezaif/tereza-coaching">Book our first call</a>
      </div>
      </div>
    </div>
  {% endif %}
{% endfor %}